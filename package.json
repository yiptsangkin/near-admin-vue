{
  "name": "near-admin-vue",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "serve:translate": "node script/translate.js && vue-cli-service serve",
    "translate": "node script/translate.js",
    "lint": "vue-cli-service lint",
    "build": "vue-cli-service build",
    "uat:build": "BUILD_ENV=uat vue-cli-service build",
    "prod:build": "BUILD_ENV=prod vue-cli-service build",
    "analyze:build": "ANALYZE=true vue-cli-service build",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit",
    "commit": "git-cz",
    "genlog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "dependencies": {
    "core-js": "^3.6.4",
    "register-service-worker": "^1.7.1",
    "vue": "^2.6.11",
    "vue-router": "^3.1.6",
    "vuex": "^3.1.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^5.2.4",
    "@types/mockjs": "^1.0.2",
    "@vue/cli-plugin-babel": "^4.3.0",
    "@vue/cli-plugin-e2e-cypress": "^4.3.0",
    "@vue/cli-plugin-pwa": "^4.3.0",
    "@vue/cli-plugin-typescript": "^4.3.0",
    "@vue/cli-plugin-unit-mocha": "^4.3.0",
    "@vue/cli-service": "^4.3.0",
    "@vue/test-utils": "1.0.0-beta.31",
    "ant-design-vue": "^1.5.4",
    "axios": "^0.19.2",
    "babel-plugin-import": "^1.13.0",
    "chai": "^4.1.2",
    "commitizen": "^4.0.5",
    "compression-webpack-plugin": "^3.1.0",
    "cz-conventional-changelog": "^3.1.0",
    "google-translate-open-api": "^1.3.2",
    "husky": "^4.2.5",
    "less": "^3.11.1",
    "less-loader": "^6.0.0",
    "lint-staged": "^10.2.1",
    "logline": "^1.1.2",
    "mockjs": "^1.1.0",
    "moment": "^2.24.0",
    "prompts": "^2.3.2",
    "sass": "^1.26.3",
    "sass-loader": "^8.0.2",
    "terser-webpack-plugin": "^2.3.6",
    "typescript": "~3.8.3",
    "vue-async-computed": "^3.8.2",
    "vue-class-component": "^7.2.3",
    "vue-i18n": "^8.17.4",
    "vue-template-compiler": "^2.6.11",
    "webpack-spritesmith": "^1.1.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
